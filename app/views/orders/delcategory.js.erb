function delcategory(){
	let prestation = document.getElementsByClassName("prestation-name-<%= @category.id %>")
	let basket = document.getElementsByClassName("basket-group-<%= @category.id %>")
	let numberOfCategories = document.getElementById("puts-number-<%= @category.name %>")
	/*countnumbre definit dans helper pour calculer le nombre de Categoriie*/
	numberOfCategories.innerHTML = <%= countnumbre(@category.name,@category.id) %> 
	/* delete the last element in dom prestation */
	if (prestation.length != 0){
		prestation[prestation.length - 1].remove()
	}
	/* delete the last element in dom basket */
	if (basket.length != 0){
		basket[basket.length - 1].remove()
	}
	/* countnumbre definit dans helper pour calculer la total des prix a payer */
	document.getElementById("js-price").innerHTML = "Somme: <%= totalprice() %> â‚¬"
}
delcategory()
