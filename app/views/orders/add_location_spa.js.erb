function addspareservationsubcategory(){
	let div = document.getElementById('information-prestation')
	let div1 = document.createElement("div")
	div1.classList.add("group-form-<%= @subcategory.id %>")
	div1.innerHTML = "<%= j render 'add_spa_reservation', subcategory: @subcategory, i: @index %>"
	div.appendChild(div1)
	document.getElementById("nombre-<%= dom_id(@subcategory) %>").innerHTML= " <%= session[:params_spa].count(@subcategory.id) %>"
	let spa = document.getElementById("panier-<%= dom_id(@subcategory) %>")
	let panier = document.getElementById("list-prestation-spa")
	if (spa){
		spa.innerHTML = "<%= j render 'add_panier_reservation_spa', subcategory: @subcategory, nombre: session[:params_spa].count(@subcategory.id) %>"
	}else{
		let divelement = document.createElement("div")
		divelement.setAttribute("id", "panier-<%= dom_id(@subcategory) %>")
		divelement.innerHTML = "<%= j render 'add_panier_reservation_spa', subcategory: @subcategory, nombre: session[:params_spa].count(@subcategory.id) %>"
		panier.appendChild(divelement)
	}
	let price = document.getElementById("prix-total")
	price.innerHTML = parseInt(price.innerHTML)+<%= @subcategory.price %>
}
addspareservationsubcategory()
